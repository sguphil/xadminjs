{
    "appenders": [
        {
            "type": "console"
        },
        {
            "type": "dateFile",
            "filename": "logs/",
            "absolute": false,
            "maxLogSize": 1024,
            "backups": 3,
            "pattern": "yyyyMMddhh.txt",
            "absolute": false,
            "alwaysIncludePattern": true
        }
    ]
}